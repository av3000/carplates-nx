{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@backend-express/carplate/controllers": [
        "libs/backend-express/carplate/controllers/src/index.ts"
      ],
      "@backend-express/carplate/routes": [
        "libs/backend-express/carplate/routes/src/index.ts"
      ],
      "@backend-express/carplate/schema": [
        "libs/backend-express/carplate/schema/src/index.ts"
      ],
      "@backend-express/utils": ["libs/backend-express/utils/src/index.ts"],
      "@frontend-angular/carplate/carplates-feature-carplate-list": [
        "libs/frontend-angular/carplate/carplate-feature-carplate-list/src/index.ts"
      ],
      "@shared/carplate/types": ["libs/shared/carplate/types/src/index.ts"],
      "@shared/common/constants": ["libs/shared/common/constants/src/index.ts"],
      "@shared/common/enums": ["libs/shared/common/enums/src/index.ts"],
      "@shared/common/types": ["libs/shared/common/types/src/index.ts"],
      "@shared/common/utils": ["libs/shared/common/utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
